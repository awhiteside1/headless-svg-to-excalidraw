{
	"name": "headless-svg-to-excalidraw",
  "version": "0.0.10",
	"description": "Convert SVG to Excalidraw's file format, no browser required.",
	"type": "module",
	"publishConfig": {
		"access": "public"
	},
  "repository": "github:awhiteside1/headless-svg-to-excalidraw",
	"bugs": {
		"url": "https://github.com/awhiteside1/headless-svg-to-excalidraw/issues"
	},
	"homepage": "https://github.com/awhiteside1/headless-svg-to-excalidraw#readme",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			}
		}
	},
	"files": [
		"dist",
		"package.json"
	],
	"scripts": {
		"build": "tsdown",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"lint": "biome check --write"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@tsconfig/bun": "^1.0.10",
		"@types/bun": "^1.3.7",
		"@types/chroma-js": "^2.1.3",
		"tsdown": "^0.20.1",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"chroma-js": "^2.1.2",
		"gl-matrix": "^3.3.0",
		"linkedom": "^0.18.12",
		"nanoid": "5.0.9",
		"path-data-parser": "^0.1.0",
		"points-on-curve": "^0.2.0",
		"points-on-path": "^0.2.1",
		"roughjs": "^4.4.1"
	}
}
